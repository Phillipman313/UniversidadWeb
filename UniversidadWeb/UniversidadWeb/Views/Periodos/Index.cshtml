@model IEnumerable<UniversidadWeb.Models.Periodo>

@{
    ViewBag.Title = "Periodos";
}

<div class="matricula-header">
    <h1>Periodos</h1>
</div>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Orden)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaInicio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaFinal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Anyo.Anyo1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ciclo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Sede)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Orden)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaInicio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaFinal)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Anyo.Anyo1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ciclo.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sede.Nombre)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id_Periodo }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id_Periodo }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id_Periodo })
        </td>
    </tr>
}

</table>
